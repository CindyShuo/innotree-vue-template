{
  "useConfigFiles": true,
  "router": true,
  "cssPreprocessor": "stylus",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save", "commit"]
    },
    "vue-cli-plugin-custom-tpl": {
      "replaceTemplates": true,
      "router": true
    }
  },
  "configs": {
    "vuex": true,
    "routerHistoryMode": true
  }
}
